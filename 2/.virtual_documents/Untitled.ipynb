class OurException(Exception):
    pass
def raise_her_exception(a):
    print(a, 'is a')
    raise OurException
    print('easygoing person.')
def func(key: int):
    try:
        if key == 0:
            raise_her_exception('Saya')
    except OurException as e:
        print('intelligent')
        raise Exception

key = 0
try:
    func(key)
except Exception as f:
    print('speedster.')



