def culc(a, b, squares=[], cubes=[]):
    squares.append(a ** 2)
    cubes.append(b ** 3)
    return squares, cubes

print(culc(2,2))
print(culc(3,3))
print('ans ', culc(4,4))
print(culc(5,5))





